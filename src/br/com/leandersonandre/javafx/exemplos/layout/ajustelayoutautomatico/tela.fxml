<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.leandersonandre.javafx.exemplos.acoes.ocultarcomponentes.Controller">

      <Label layoutX="22.0" layoutY="26.0" prefHeight="17.0" prefWidth="167.0" text="Ocultar componente" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets left="10.0" right="10.0" top="10.0" />
         </VBox.margin></Label>
      <TextField fx:id="input">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin></TextField>
      <CheckBox fx:id="checkBox" layoutY="109.0" mnemonicParsing="false" onAction="#onButtonClick" selected="true" text="Exibir" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets bottom="10.0" right="10.0" />
         </VBox.margin></CheckBox>
   <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Button" VBox.vgrow="ALWAYS">
      <VBox.margin>
         <Insets bottom="10.0" left="10.0" right="10.0" />
      </VBox.margin>
   </Button>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>

</VBox>
